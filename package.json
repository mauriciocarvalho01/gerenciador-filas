  "license": "GPL-3.0-or-later",
  "keywords": [
    "clean architecture",
    "clean code",
    "tdd",
    "ddd",
    "solid",
    "design patterns",
    "node",
    "typescript",
    "jest"
  ],
  "author": {
    "name": "Mauricio Carvalho",
    "email": "developer.mauricio1@gmail.com"
