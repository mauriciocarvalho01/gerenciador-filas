    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "multer": "^1.4.3",
    "node-cron": "^3.0.2",
    "node-fetch": "^3.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@jest-mock/express": "^1.4.4",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/md5": "^2.3.2",
    "@types/module-alias": "^2.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.7.13",
    "@types/node-cron": "^3.0.7",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "coveralls": "^3.1.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.2",
    "jest": "^27.1.1",
    "jest-mock-extended": "^2.0.1",
    "lint-staged": "^11.1.2",
    "mockdate": "^3.0.5",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.2"
  },
  "engines": {
    "node": "^19.x"
  }
}
